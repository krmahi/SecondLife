        "apiKey": "4c96e79a93549fc42a60002fa95eee73"
      }
    },
    "owner": "mahi_kr",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d0f4a173-2a5d-43c3-a7bf-3030a424a6a2"
